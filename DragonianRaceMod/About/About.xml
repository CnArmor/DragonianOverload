      <steamWorkshopUrl>https://steamcommunity.com/sharedfiles/filedetails/?id=839005762</steamWorkshopUrl>
    </li>
    <li>
      <packageId>brrainz.harmony</packageId>
      <displayName>Harmony</displayName>
      <steamWorkshopUrl>steam://url/CommunityFilePage/2009463077</steamWorkshopUrl>
      <downloadUrl>https://github.com/pardeike/HarmonyRimWorld/releases/latest</downloadUrl>
    </li>
  </modDependencies>
  <loadAfter>
    <li>brrainz.harmony</li>
    <li>Ludeon.RimWorld</li>
    <li>erdelf.HumanoidAlienRaces</li>
  </loadAfter>
  <loadBefore>
    <li>ApertureStaff.DragonianRaceFactions</li>
    <li>ApertureStaff.DragonianRaceApparelPatch</li>
    <li>ApertureStaff.DragonianRaceWeapons</li>
